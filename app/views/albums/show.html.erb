<div id="post">
  <% @album.images.each do |image| %>
    <div id="image">
      <%= image_tag(image.image.url(:medium)) %>
    </div>

    <div class="description">
      <%=h image.description %>
    </div>
    
    <div id="user_tags">
      <% image.user_tags.each do |tag| %>
        <%= twitter_profile_link(tag.twitter_login) %>
      <% end %>
    </div>
  <% end %>
</div>

<div>
  <% link_to edit_album_url(@album) do %>
    Add photos to this album
  <% end %>
</div>
